  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/server.ts",
    "typeorm": "ts-node-dev node_modules/typeorm/cli.js",
    "start": "node dist/server.js",
    "start:docker:dev": "npm install && npm run dev",
    "test": "jest"
